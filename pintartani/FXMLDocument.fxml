<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="364.0" prefWidth="505.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pintartani.FXMLDocumentController">
   <children>
      <ImageView fitHeight="455.0" fitWidth="640.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Gambar/LoginForm.png" />
         </image>
      </ImageView>
      <Button fx:id="button" layoutX="455.0" layoutY="330.0" onAction="#handleButtonMasuk" prefHeight="40.0" prefWidth="98.0" style="-fx-background-color: #4ECC53; -fx-background-radius: 12,5;" text="MASUK" textFill="WHITE">
         <font>
            <Font name="Roboto Bold" size="16.0" />
         </font>
      </Button>
      <TextField fx:id="user" layoutX="404.0" layoutY="194.0" prefHeight="40.0" prefWidth="200.0" promptText="Username" style="-fx-border-color: #4ECC53; -fx-border-radius: 15; -fx-border-width: 2; -fx-background-radius: 15;" />
      <PasswordField fx:id="pass" layoutX="404.0" layoutY="256.0" prefHeight="40.0" prefWidth="200.0" promptText="Password" style="-fx-background-radius: 15; -fx-border-color: #4ECC53; -fx-border-radius: 15; -fx-border-width: 2;" />
      <Button layoutX="530.0" layoutY="400.0" mnemonicParsing="false" onAction="#handleButtonDaftar" style="-fx-background-color: transparent;" text="Daftar" textFill="#4ecc53">
         <font>
            <Font name="Roboto Bold" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
